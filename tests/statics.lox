class Math {
    static pi(){
        return 3.1415;
    }
    method(){
        print "method!";
    }
}

var a = Math.pi;
print a();                 // Expect: 3.1415

var b = Math();
b.method();                // Expect: method!
// b.pi();                 // Expect: compile error

print Math.pi();           // Expect: 3.1415
// Math.method();          // Expect: runtime error

class Derived < Math {
    /*
    static pi(){
        // print this;     // Expect: compile error
        print super.pi;
        print super.pi();
    }
    static demo(){
        print super.method; // Expect: compile error
    }
    */
    nonstatic(){
        print super.method;
        print super.pi;
    }
}
